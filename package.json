{
  "name": "hyperterm",
  "productName": "HyperTerm",
  "version": "0.4.5",
  "license": "MIT",
  "repository": "zeit/hyperterm",
  "description": "HTML/JS/CSS Terminal",
  "dependencies": {
    "child_pty": "3.0.1",
    "convert-css-color-name-to-hex": "0.1.1",
    "default-shell": "1.0.1",
    "electron-config": "0.2.0",
    "electron-is-dev": "0.1.1",
    "gaze": "1.1.0",
    "mkdirp": "0.5.1",
    "ms": "0.7.1",
    "uid2": "0.0.3"
  },
  "devDependencies": {
    "electron-packager": "7.1.0",
    "electron-prebuilt": "1.2.5",
    "eslint": "2.13.1",
    "eslint-config-standard": "5.3.1",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-standard": "1.3.2"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "yoda": 0,
      "semi": [
        2,
        "always"
      ],
      "no-unused-vars": 2,
      "no-extra-semi": 2,
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ]
    }
  },
  "scripts": {
    "start": "electron index",
    "lint": "eslint *.js"
  }
}
