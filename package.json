{
  "name": "hyperterm",
  "productName": "HyperTerm",
  "version": "0.6.0",
  "license": "MIT",
  "repository": "zeit/hyperterm",
  "description": "HTML/JS/CSS Terminal",
  "dependencies": {
    "child_pty": "3.0.1",
    "convert-css-color-name-to-hex": "0.1.1",
    "default-shell": "1.0.1",
    "electron-config": "0.2.1",
    "electron-is-dev": "0.1.1",
    "gaze": "1.1.0",
    "mkdirp": "0.5.1",
    "ms": "0.7.1",
    "shell-env": "0.1.2",
    "uid2": "0.0.3"
  },
  "devDependencies": {
    "electron-packager": "7.3.0",
    "electron-prebuilt": "1.2.7",
    "eslint": "3.1.1",
    "eslint-config-standard": "5.3.5",
    "eslint-plugin-promise": "2.0.0",
    "eslint-plugin-react": "5.2.2",
    "eslint-plugin-standard": "2.0.0",
    "husky": "0.11.4"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "yoda": 0,
      "semi": [
        2,
        "always"
      ],
      "no-unused-vars": 2,
      "no-extra-semi": 2,
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ]
    }
  },
  "scripts": {
    "start": "electron index",
    "lint": "eslint .",
    "prepublish": "npm test",
    "prepush": "npm test",
    "test": "npm run lint"
  }
}
